{
    "name": "ivoglent/yii2-elastic-apm",
    "description": "Elastic Agent for Yii2 Framework",
    "type": "module",
    "version": "1.0.8",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "ivoglent/elastic-apm-agent-v2": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ivoglent\\yii2\\apm\\": "./src"
        }
    },
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/guigpm/elastic-apm-agent-v2/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "authors": [
        {
            "name": "Ivoglent Nguyen",
            "email": "ivoglent@gmail.com"
        }
    ],
    "prefer-stable": true
}
